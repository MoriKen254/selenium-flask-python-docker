[run]
source = src
omit =
    tests/*
    */tests/*
    venv/*
    */site-packages/*
    */dist-packages/*
    */htmlcov/*
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
